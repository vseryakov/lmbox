# 
# /etc/fstab: static file system information
#
# <file system>        <dir>         <type>    <options>          <dump> <pass>
none                   /dev/pts      devpts    defaults            0      0
none                   /dev/shm      tmpfs     defaults            0      0
usbfs                  /proc/bus/usb usbfs  defaults            0      0

/dev/cdroms/cdrom0     /mnt/cd   iso9660   ro,user,noauto,unhide   0      0
/dev/cdroms/cdrom0     /mnt/dvd  iso9660   ro,user,noauto,unhide   0      0
/dev/floppy/0          /mnt/fl   vfat      user,noauto             0      0

/dev/hda2 swap swap defaults 0 0
/dev/hda1 / ext2 defaults 0 1

/dev/hde2 /disk1 ext2 noatime 0 1
/dev/hdf1 /disk2 ext2 noatime 0 1
/dev/hdc1 /disk3 ext2 noatime 0 1 
/dev/hdd1 /disk4 ext2 noatime 0 1 
/dev/hdb1 /disk5 ext2 noatime 0 1 

