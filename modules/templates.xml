          pressedcolor="#5983e1"
          disabledcolor="#a9ab78"
          fontcolor="#e0e2e5"
          fontfocusedcolor="#1a2d5b"
          textalign="center">

          <event name="yellow">
            lmbox call this setfocus
          </event>

          <event name="OnInit">
            lmbox set this text Filter
          </event>

          <event name="OnAdd">
            lmbox create this mosaic_prefix
            lmbox set this mosaic_prefix [lindex [split [lmbox get this parentname] _] 0]
          </event>

          <event name="OnGotFocus">
            lmbox set status caption "Enter search filter"
            if { [lmbox get this text] == "Filter" } {
              lmbox set this text ""
            }
          </event>

          <event name="OnLostFocus">
            if { [lmbox get this text] == "" } {
              lmbox set this text Filter
            }
          </event>

          <event name="OnChange">
            lmbox_mosaic [lmbox get this mosaic_prefix] init [lmbox get this name]
          </event>
</template>

